{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": true,
        "outDir": "lib",
        "lib": [
            "es2015",
            "es5"
        ],
        "sourceMap": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
    },
    "include": [
        "src/**/*.ts"
      ],
    "exclude": [
    "node_modules"
    ]
}
